      / /Services Section
      / Pricing Section
      %section#pricing.pricing.section
        / Section Title
        .container.section-title{"data-aos" => "fade-up"}
          %h2 Pricing
          %p Check my adorable pricing
        / End Section Title
        .container{"data-aos" => "fade-up", "data-aos-delay" => "100"}
          .row.gy-4.gx-lg-5
            .col-lg-6
              .pricing-item.d-flex.justify-content-between
                %h3 Portrait Photography
                %h4 $160.00
            / End Pricing Item
            .col-lg-6
              .pricing-item.d-flex.justify-content-between
                %h3 Fashion Photography
                %h4 $300.00
            / End Pricing Item
            .col-lg-6
              .pricing-item.d-flex.justify-content-between
                %h3 Sports Photography
                %h4 $200.00
            / End Pricing Item
            .col-lg-6
              .pricing-item.d-flex.justify-content-between
                %h3 Still Life Photography
                %h4 $120.00
            / End Pricing Item
            .col-lg-6
              .pricing-item.d-flex.justify-content-between
                %h3 Wedding Photography
                %h4 $500.00
            / End Pricing Item
            .col-lg-6
              .pricing-item.d-flex.justify-content-between
                %h3 Photojournalism
                %h4 $200.00
            / End Pricing Item
      / /Pricing Section
